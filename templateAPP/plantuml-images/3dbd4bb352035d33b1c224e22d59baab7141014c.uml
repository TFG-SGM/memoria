@startuml

entity Admin{
_id : string
name : string
surname : string
bornDate : date
address : string 
email : string
phone : number
password : string
}

entity Doctor{
_id : string
name : string
surname : string
bornDate : date
address : string 
email : string
phone : number
password : string
}

entity Patient{
_id : string
name : string
surname : string
bornDate : date
address : string 
email : string
phone : number
}

entity Test {
_id : string
doctor : Doctor
date : date
type : string
patientId: string
videoFile : file
}

entity TestData{
_id : string
testId: string
...data
}

Doctor --o Test
Patient o-- Test
Test *-- TestData
@enduml