    void get("/patient/:id")
    void put("/patient/:id")
    void delete("/patient/:id")
}

class TestRouter #LightGrey {
    void get("/test/")
    void post("/test/")
    void get("/test/:id")
    void put("/test/:id")
    void delete("/test/:id")
    void delete("/test/patient/:patientId")
